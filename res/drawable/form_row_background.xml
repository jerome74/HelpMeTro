<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_last="true">
        <layer-list
          xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="1.0px" android:color="#ffaaaaaa" />
                    <solid android:color="#ffffffff" />
                </shape>
            </item>
            <item android:left="1.0px" android:right="1.0px">
                <shape android:shape="rectangle">
                    <stroke android:width="1.0px" android:color="#ffffffff" />
                    <solid android:color="#00ffffff" />
                    <padding android:left="10.0dip" android:top="1.0dip" android:right="10.0dip" android:bottom="1.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list
          xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape android:shape="rectangle">
                    <stroke android:width="1.0px" android:color="#ffaaaaaa" />
                    <solid android:color="#ffffffff" />
                </shape>
            </item>
            <item android:left="1.0px" android:right="1.0px" android:bottom="1.0px">
                <shape android:shape="rectangle">
                    <stroke android:width="1.0px" android:color="#ffffffff" />
                    <solid android:color="#00ffffff" />
                    <padding android:left="10.0dip" android:top="1.0dip" android:right="10.0dip" android:bottom="1.0dip" />
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
